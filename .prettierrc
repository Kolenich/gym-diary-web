{
  "semi": true,
  "printWidth": 120,
  "tabWidth": 2,
  "singleQuote": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "useTabs": false,
  "arrowParens": "always",
  "jsxSingleQuote": true,
  "trailingComma": "all",
  "endOfLine": "auto",
  "importOrder": [
    "^api(.*)$",
    "^components(.*)$",
    "^constants(.*)$",
    "^pages(.*)$",
    "^routers(.*)$",
    "^store(.*)$",
    "^utils(.*)$",
    "^[../]",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "order": "alphabetical"
}
