{
  "name": "gym_diary",
  "version": "0.3.1",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.2.1",
    "@mui/material": "^6.2.1",
    "@reduxjs/toolkit": "^2.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.0.2",
    "uuid": "^11.0.3"
  },
  "scripts": {
    "start": "webpack serve --config __webpack__/dev.config.ts",
    "build": "webpack --config __webpack__/prod.config.ts",
    "commit:patch": "npm version patch --no-git-tag-version --force && git add .",
    "commit:minor": "npm version minor --no-git-tag-version --force && git add .",
    "commit:major": "npm version major --no-git-tag-version --force && git add .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --cache --fix"
    ],
    "!(**/*.css.d).{js,jsx,ts,tsx,md,html,css,scss,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/circular-dependency-plugin": "^5.0.8",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "babel-loader": "^9.2.1",
    "circular-dependency-plugin": "^5.2.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.4.2",
    "react-refresh": "^0.16.0",
    "terser-webpack-plugin": "^5.3.11",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.0",
    "webpack-merge": "^6.0.1"
  }
}
